{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|html|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/about",
      "dest": "/About.html"
    },
    {
      "src": "/contact",
      "dest": "/Contact.html"
    },
    {
      "src": "/courses",
      "dest": "/courses.html"
    },
    {
      "src": "/faq",
      "dest": "/FAQs.html"
    },
    {
      "src": "/support",
      "dest": "/Support.html"
    },
    {
      "src": "/blog",
      "dest": "/Blog.html"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}