{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|html|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/about",
      "dest": "/About.html"
    },
    {
      "src": "/contact",
      "dest": "/Contact.html"
    },
    {
      "src": "/courses",
      "dest": "/courses.html"
    },
    {
      "src": "/faq",
      "dest": "/FAQs.html"
    },
    {
      "src": "/support",
      "dest": "/Support.html"
    },
    {
      "src": "/blog",
      "dest": "/Blog.html"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}